# OpenRouter API for evolution (free tier)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Hugging Face Hub for model downloads
HF_TOKEN=your_huggingface_token_here

# Model Configuration
DEEPSEEK_MODEL_PATH=./data/models/DeepSeek-Prover-V2-7B-Q4_K_M.gguf
DEEPSEEK_MODEL_REPO=unsloth/DeepSeek-Prover-V2-7B-GGUF

# Hardware Settings
MAX_RAM_GB=12
CPU_THREADS=8
GPU_ENABLED=false

# Inference Settings
